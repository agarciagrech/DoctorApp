<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="doctorapp.ShowSignalsController">
   <children>
      <TextField fx:id="txtFIlename" layoutX="206.0" layoutY="187.0" promptText="Signal Filename" />
      <Text layoutX="171.0" layoutY="43.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Introduce the medical card of the patient:" />
      <Button fx:id="backButton" layoutX="45.0" layoutY="363.0" mnemonicParsing="false" onAction="#back" text="Back " />
      <Button fx:id="consultPatients" layoutX="67.0" layoutY="69.0" mnemonicParsing="false" onAction="#showPatients" text="Consult Patients" />
      <TextField fx:id="txtMedCard" layoutX="206.0" layoutY="69.0" promptText="Medical card" />
      <Text layoutX="193.0" layoutY="166.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Introduce the name of the signal:" />
      <Button fx:id="consultFilenames" layoutX="67.0" layoutY="187.0" mnemonicParsing="false" onAction="#showFilenames" text="Consult Filenames " />
      <Button fx:id="showButton" layoutX="67.0" layoutY="241.0" mnemonicParsing="false" onAction="#showSignal" text="Show signal" />
      <Button fx:id="exitButton" layoutX="503.0" layoutY="363.0" mnemonicParsing="false" onAction="#exitApp" text="Exit" />
   </children>
</AnchorPane>
